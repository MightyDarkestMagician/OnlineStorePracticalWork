@model IEnumerable<OnlineStorePracticalWork.Models.Order>

@{
    ViewBag.Title = "Мои заказы";
}

<h2>Мои заказы</h2>

<table class="table">
    <thead>
        <tr>
            <th>Дата заказа</th>
            <th>Общая сумма</th>
            <th>Статус</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var order in Model)
        {
            <tr>
                <td>@order.OrderDate</td>
                <td>@order.TotalPrice</td>
                <td>@order.Status</td>
            </tr>
        }
    </tbody>
</table>
