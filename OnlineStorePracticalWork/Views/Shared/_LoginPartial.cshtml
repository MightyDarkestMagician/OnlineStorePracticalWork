            @Html.ActionLink("Личный Кабинет", "Manage", "Account", null, new { @class = "nav-link" })
        </li>
        <li class="nav-item">
            @Html.ActionLink("Корзина", "Index", "Cart", null, new { @class = "nav-link" })
