@model OnlineStorePracticalWork.Models.Product

@{
    ViewBag.Title = "Детали продукта";
}

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <h2>@Model.Name</h2>
            <p>@Model.Description</p>
            <p>Цена: @Model.Price</p>
            <p>Количество на складе: @Model.Stock</p>
        </div>
    </div>
</div>
